<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/subscribe_item_selected_stroke</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--黑夜主题-->
    <style name="AppTheme.Black" parent="Theme.AppCompat.NoActionBar"></style>


    <style name="jili">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="chicun">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:drawablePadding">1.0dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:textColor">@color/hei</item>
        <item name="android:textSize">16.0sp</item>
    </style>

    <style name="ziti">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:drawablePadding">1.0dip</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">10.0dip</item>

        <item name="android:textSize">16.0sp</item>
    </style>

    <style name="xuxian">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2.0dip</item>
        <item name="android:background">@mipmap/biz_pc_account_line</item>
    </style>

    <style name="tupian">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>

    <style name="jiahaoyanse">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:background">#fff</item>
    </style>

    <style name="lin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="rdiobutton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="titleziti">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">21sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="youxiangzhuce">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="youxiangzhuce01">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="youxiangzhuce02">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@null</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="lin_buju">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="biaoshi">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="tuichuzi">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:textColor">@color/subscribe_item_selected_stroke</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="tuichubiaoshiziti">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15dp</item>
    </style>
</resources>
